global:
    scrape_interval: 15s

scrape_configs:
    -   job_name: 'prometheus'
        scrape_interval: 60s
        static_configs:
            -   targets: ['localhost:9090']
        

    -   job_name: '5e.jafner.tools'
        scrape_interval: 60s
        static_configs:
            -   targets: ['5e-jafner-tools:9117','https://5e.jafner.tools']

    -   job_name: 'pihole.jafner.net'
        scrape_interval: 60s
        static_configs:
            -   targets: ['pihole-jafner-net:9617','http://pihole.jafner.net']

    -   job_name: 'uptime-kuma'
        scrape_interval: 60s
        static_configs:
            -   targets: ['uptime.jafner.net']
        basic_auth:
            username: Jafner
            password: ***REMOVED***
