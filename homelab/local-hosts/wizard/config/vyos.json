    },
    "source": {
      "rule": {
        "99": {
          "description": "Masquerade as public IP on internet",
          "outbound-interface": {
            "name": "eth5"
          },
          "source": {
            "address": "192.168.1.0/24"
          },
          "translation": {
            "address": "masquerade"
          }
        },
        "100": {
          "description": "NAT Reflection",
          "destination": {
            "address": "192.168.1.0/24"
          },
          "outbound-interface": {
            "name": "eth6"
          },
          "protocol": "tcp_udp",
          "source": {
            "address": "192.168.1.0/24"
          },
          "translation": {
            "address": "masquerade"
          }
        }
      }
    }
  },
